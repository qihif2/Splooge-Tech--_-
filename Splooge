do
local a=true
local b=Enum.KeyCode.F
local c=Enum.KeyCode.RightShift
local d=7
local e,f=1,20
local g=2
local h,i=1,6
local j=0
local k,l=0,1
local m=true
local n=true
 
local o=game:GetService("Players")
local p=o.LocalPlayer
local q=p.Character or p.CharacterAdded:Wait()
local r=q:WaitForChild("HumanoidRootPart")
local s=game:GetService("UserInputService")
local t=game:GetService("TweenService")
 
local u,v
 
s.InputBegan:Connect(function(x,y)
 if y then return end
 if x.KeyCode==b then
  a=not a
  if u then u() end
 elseif x.KeyCode==c and _G.FlingUI then
  _G.FlingUI.Enabled=not _G.FlingUI.Enabled
 end
end)
 
local function w()
 if not a then return end
 r.Velocity=r.CFrame.LookVector*d*50+Vector3.new(0,d*50,0)
end
 
local function x(y)
 return y.Name=="LeftFoot" or y.Name=="RightFoot" or y.Name=="Left Leg" or y.Name=="Right Leg"
end
 
for _,y in ipairs(q:GetChildren()) do
 if y:IsA("BasePart") and x(y) then
  y.Touched:Connect(function(z)
   if a and z and z.Name=="Head" then
    local A=o:GetPlayerFromCharacter(z.Parent)
    if A and A~=p then
     w()
    end
   end
  end)
 end
end
 
local B={}
 
local function C()
 for _,D in ipairs(o:GetPlayers()) do
  if D~=p and D.Character then
   local E=D.Character:FindFirstChild("Head")
   if E then
    if not B[E] then
     B[E]={Size=E.Size,Transparency=E.Transparency}
    end
    if m then
     E.Size=Vector3.new(g,B[E].Size.Y,g)
    else
     E.Size=B[E].Size
    end
    if m and n then
     E.Transparency=j
    else
     E.Transparency=B[E].Transparency
    end
    E.Massless=true
   end
  end
 end
end
 
o.PlayerAdded:Connect(function(F)
 F.CharacterAdded:Connect(C)
end)
 
local G=Instance.new("ScreenGui",p.PlayerGui)
G.Name="FlingUI"
G.ResetOnSpawn=false
G.ZIndexBehavior=Enum.ZIndexBehavior.Sibling
_G.FlingUI=G
 
local H=Instance.new("Frame",G)
H.Size=UDim2.fromScale(.15,.28)+UDim2.fromOffset(8,8)
H.AnchorPoint=Vector2.new(.5,.5)
H.Position=UDim2.fromScale(.5,.5)
H.BackgroundColor3=Color3.fromRGB(130,50,220)
H.BackgroundTransparency=.85
H.BorderSizePixel=0
Instance.new("UICorner",H).CornerRadius=UDim.new(0,16)
t:Create(H,TweenInfo.new(1.5,Enum.EasingStyle.Sine,Enum.EasingDirection.InOut,-1,true),{BackgroundTransparency=.8}):Play()
 
local I=Instance.new("Frame",G)
I.Size=UDim2.fromScale(.15,.28)
I.AnchorPoint=Vector2.new(.5,.5)
I.Position=UDim2.fromScale(.5,.5)
I.BackgroundColor3=Color3.fromRGB(25,25,30)
I.BorderSizePixel=0
Instance.new("UICorner",I).CornerRadius=UDim.new(0,16)
I.ZIndex=H.ZIndex+1
 
local J=false
local K,L,M
 
local function N(O)
 local P=O.Position-L
 I.Position=UDim2.new(0,M.X+P.X,0,M.Y+P.Y)
end
 
I.InputBegan:Connect(function(O)
 if O.UserInputType==Enum.UserInputType.MouseButton1 then
  J=true
  L=O.Position
  M=I.Position
  O.Changed:Connect(function()
   if O.UserInputState==Enum.UserInputState.End then
    J=false
   end
  end)
 end
end)
 
I.InputChanged:Connect(function(O)
 if O.UserInputType==Enum.UserInputType.MouseMovement then
  K=O
 end
end)
 
s.InputChanged:Connect(function(O)
 if O==K and J then
  N(O)
 end
end)
 
local Q=Instance.new("UIStroke",I)
Q.Color=Color3.fromRGB(0,0,0)
Q.Thickness=2
Q.Transparency=.5
 
local function R(S,T)
 local U=Instance.new("TextLabel",I)
 U.Position=UDim2.fromScale(.05,T)
 U.Size=UDim2.fromScale(.9,.05)
 U.Text=S
 U.TextScaled=true
 U.Font=Enum.Font.GothamBold
 U.TextColor3=Color3.fromRGB(230,220,255)
 U.BackgroundTransparency=1
 return U
end
 
local function V(T,W,X,Y)
 local Z=Instance.new("Frame",I)
 Z.Position=UDim2.fromScale(.08,T)
 Z.Size=UDim2.fromScale(.84,.045)
 Z.BackgroundColor3=Color3.fromRGB(40,40,50)
 Z.BorderSizePixel=0
 Instance.new("UICorner",Z).CornerRadius=UDim.new(1,0)
 
 local a0=Instance.new("Frame",Z)
 a0.BackgroundColor3=Color3.fromRGB(180,80,255)
 a0.BorderSizePixel=0
 Instance.new("UICorner",a0).CornerRadius=UDim.new(1,0)
 
 local a1=Instance.new("Frame",Z)
 a1.Size=UDim2.fromScale(.04,1)
 a1.AnchorPoint=Vector2.new(.5,0)
 a1.BackgroundColor3=Color3.fromRGB(230,220,255)
 a1.BorderSizePixel=0
 Instance.new("UICorner",a1).CornerRadius=UDim.new(1,0)
 
 local a2=Instance.new("TextLabel",I)
 a2.Position=UDim2.fromScale(.88,T-.005)
 a2.Size=UDim2.fromScale(.12,.045)
 a2.Font=Enum.Font.Gotham
 a2.TextColor3=Color3.fromRGB(230,220,255)
 a2.BackgroundTransparency=1
 a2.TextScaled=true
 
 local a3=false
 local a4=TweenInfo.new(.15,Enum.EasingStyle.Quad,Enum.EasingDirection.Out)
 
 local function a5(a6)
  local a7=math.clamp((a6-Z.AbsolutePosition.X)/Z.AbsoluteSize.X,0,1)
  t:Create(a0,a4,{Size=UDim2.fromScale(a7,1)}):Play()
  t:Create(a1,a4,{Position=UDim2.fromScale(a7,0)}):Play()
  Y(W+(X-W)*a7)
  a2.Text=string.format("%.2f",Y())
 end
 
 Z.InputBegan:Connect(function(a6)
  if a6.UserInputType==Enum.UserInputType.MouseButton1 then
   a3=true
   a5(a6.Position.X)
  end
 end)
 
 s.InputChanged:Connect(function(a6)
  if a3 and a6.UserInputType==Enum.UserInputType.MouseMovement then
   a5(a6.Position.X)
  end
 end)
 
 s.InputEnded:Connect(function(a6)
  if a6.UserInputType==Enum.UserInputType.MouseButton1 then
   a3=false
  end
 end)
 
 a0.Size=UDim2.fromScale((Y()-W)/(X-W),1)
 a1.Position=UDim2.fromScale((Y()-W)/(X-W),0)
end
 
local a8=Instance.new("TextButton",I)
a8.Position=UDim2.fromScale(.12,.02)
a8.Size=UDim2.fromScale(.76,.06)
a8.Font=Enum.Font.GothamBold
a8.TextScaled=true
a8.TextColor3=Color3.fromRGB(230,220,255)
a8.BorderSizePixel=0
Instance.new("UICorner",a8).CornerRadius=UDim.new(0,12)
 
local a9=TweenInfo.new(.15,Enum.EasingStyle.Quad,Enum.EasingDirection.Out)
 
u=function()
 a8.Text="Fling: "..(a and "ON" or "OFF")
 a8.BackgroundColor3=a and Color3.fromRGB(130,50,220) or Color3.fromRGB(90,0,120)
end
 
a8.MouseButton1Click:Connect(function()
 a=not a
 u()
end)
 
u()
 
R("Fling Power",.1)
V(.16,e,f,function(v) if v then d=math.floor(v) end return d end)
 
R("Head Width",.28)
V(.34,h,i,function(v) if v then g=math.floor(v*10)/10 C() end return g end)
 
R("Head Transparency",.46)
V(.52,k,l,function(v) if v then j=math.floor(v*100)/100 C() end return j end)
 
local function b0(S,T,Y)
 local b1=Instance.new("TextButton",I)
 b1.Position=UDim2.fromScale(.12,T)
 b1.Size=UDim2.fromScale(.76,.055)
 b1.Font=Enum.Font.GothamBold
 b1.TextScaled=true
 b1.TextColor3=Color3.fromRGB(230,220,255)
 b1.BorderSizePixel=0
 Instance.new("UICorner",b1).CornerRadius=UDim.new(0,12)
 
 local function b2()
  local b3=Y()
  b1.Text=S..": "..(b3 and "ON" or "OFF")
  b1.BackgroundColor3=b3 and Color3.fromRGB(130,50,220) or Color3.fromRGB(90,0,120)
 end
 
 b1.MouseButton1Click:Connect(function()
  Y(true)
  b2()
  C()
 end)
 
 b2()
 return b2
end
 
v=b0("Head Size",.64,function(z)
 if z then
  m=not m
  if not m then n=false end
 end
 return m
end)
 
b0("Transparency",.72,function(z)
 if z and m then n=not n end
 return n
end)
 
R("F = Toggle | RightShift = Menu",.8)
R("Made by splooge",.85)
 
C()
end
